{
    "window.zoomLevel": 1,
    "editor.fontFamily": "Inconsolata,  Hiragino Sans",
    "editor.lineHeight": 15,
    "editor.fontWeight": "500",
    "editor.tabSize": 4,
    "editor.detectIndentation": false,
    "editor.insertSpaces": true,
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "none",
    "editor.minimap.enabled": false,
    "editor.glyphMargin": false,
    "editor.renderLineHighlight": "none",
    "editor.cursorBlinking": "solid",
    "editor.renderIndentGuides": false,
    "workbench.colorTheme": "Monokai",
    "workbench.activityBar.visible": false,
    "workbench.sideBar.location": "left",
    "workbench.list.keyboardNavigation": "filter",
    "workbench.statusBar.visible": true,
    "workbench.iconTheme": null,
    "workbench.editor.enablePreview": false,
    "workbench.editor.tabCloseButton": "off",
    "explorer.openEditors.visible": 0,
    "explorer.decorations.colors": false,
    "diffEditor.renderSideBySide": false,
    "markdown.preview.lineHeight": 1.2,
    "breadcrumbs.enabled": false,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<tab>"
            ],
            "commands": [
                "workbench.action.focusSideBar"
            ]
        },
        {
            "before": [
                ","
            ],
            "commands": [
                "workbench.action.nextEditor",
                "extension.vim_escape"
            ]
        },
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "workbench.action.toggleSidebarVisibility"
            ]
        },
        {
            "before": [
                "<left>"
            ],
            "commands": [
                {
                    "command": "editor.fold"
                }
            ]
        },
        {
            "before": [
                "<right>"
            ],
            "commands": [
                {
                    "command": "editor.unfold"
                }
            ]
        },
        {
            "before": [
                "="
            ],
            "commands": [
                {
                    "command": "editor.action.formatDocument"
                }
            ]
        },
        {
            "before": [
                "/"
            ],
            "commands": [
                {
                    "command": "editor.action.inspectTMScopes"
                }
            ]
        }
    ],
    "vim.useSystemClipboard": true,
    "vim.leader": "<space>",
    "vim.foldfix": true,
    "zenMode.hideTabs": false,
    "zenMode.centerLayout": false,
    "macros": {
        "openFileAndCloseSidebar": [
            "list.select",
            "workbench.action.toggleSidebarVisibility"
        ],
        "focusRightGroupAndCloseSidebar": [
            "workbench.action.focusRightGroup",
            "workbench.action.toggleSidebarVisibility"
        ],
    },
    "scm.diffDecorationsGutterWidth": 1,
    "diffEditor.renderIndicators": false,
    "scm.diffDecorations": "none",
    "git.openDiffOnClick": false,
    "window.nativeTabs": true,
    "workbench.editor.tabSizing": "shrink",
    "search.exclude": {
        "**/.git": true,
        "**/node_modules": true,
    },
    "javascript.updateImportsOnFileMove.enabled": "never",
    "editor.tokenColorCustomizations": {
        "[Monokai]": {
            "comments": "#888",
            "textMateRules": [
                {
                    "scope": [
                        "meta.object-literal.key",
                    ],
                    "settings": {
                        "foreground": "#E6DB74"
                    }
                },
                {
                    "scope": [
                        "storage.type.js",
                        "support.type.property-name.css",
                        "support.type.property-name.json",
                    ],
                    "settings": {
                        "fontStyle": ""
                    }
                },
            ]
        }
    },
    "workbench.colorCustomizations": {
        "[Monokai]": {
            "tab.inactiveBackground": "#121212",
            "tab.inactiveForeground": "#bbb",
            "tab.activeBackground": "#70a038",
            "tab.activeForeground": "#121212",
            "editorGroupHeader.tabsBackground": "#000",
            "editor.background": "#121212",
            "sideBar.background": "#121212",
            "sideBar.foreground": "#ddd",
            "sideBar.border": "#643",
            "statusBar.background": "#282828",
            "statusBar.foreground": "#ccc",
            "sideBarSectionHeader.background": "#000",
        }
    },
    "editor.fontSize": 15,
}