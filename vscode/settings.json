{
    "window.zoomLevel": 0.8,
    "editor.fontFamily": "Inconsolata,  Hiragino Sans",
    "editor.lineHeight": 15,
    "editor.fontWeight": "500",
    "editor.tabSize": 4,
    "editor.detectIndentation": false,
    "editor.insertSpaces": true,
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "none",
    "editor.minimap.enabled": false,
    "editor.glyphMargin": false,
    "editor.renderLineHighlight": "none",
    "editor.cursorBlinking": "solid",
    "editor.renderIndentGuides": false,
    "workbench.colorTheme": "Monokai",
    "workbench.sideBar.location": "left",
    "workbench.list.keyboardNavigation": "filter",
    "workbench.editor.enablePreview": false,
    "workbench.editor.tabCloseButton": "off",
    "explorer.openEditors.visible": 0,
    "diffEditor.renderSideBySide": false,
    "markdown.preview.lineHeight": 1.2,
    "breadcrumbs.enabled": false,
    "editor.colorDecorators": false,
    "vim.normalModeKeyBindingsNonRecursive": [
        {  "before": ["<tab>"],
           "commands": ["workbench.action.focusSideBar"]
        },
        {  "before": ["<space>"],
           "commands": ["workbench.action.toggleSidebarVisibility"]
        },
        {  "before": ["<leader>", "k"],
           "commands": ["extension.go-to-export"]
        },
        {  "before": ["<leader>", "<leader>"],
           "commands": ["workbench.action.navigateBack"]
        },
        {  "before": ["<left>"],
           "commands": [{"command": "editor.fold"}]
        },
        {  "before": ["<right>"],
           "commands": [{"command": "editor.unfold"}]
        },
        {  "before": ["="],
            "commands": [{"command": "editor.action.formatDocument"}]
        },
        {  "before": ["/"],
            "commands": [{"command": "editor.action.inspectTMScopes"}]
        }
    ],
    "vim.useSystemClipboard": true,
    "vim.leader": ",",
    "vim.foldfix": true,
    "zenMode.hideTabs": false,
    "zenMode.centerLayout": false,
    "macros": {
        "openFileAndCloseSidebar": [
            "list.select",
            "workbench.action.toggleSidebarVisibility"
        ],
        "focusRightGroupAndCloseSidebar": [
            "workbench.action.focusRightGroup",
            "workbench.action.toggleSidebarVisibility"
        ],
    },
    "scm.diffDecorationsGutterWidth": 1,
    "diffEditor.renderIndicators": false,
    "scm.diffDecorations": "none",
    "git.openDiffOnClick": false,
    "window.nativeTabs": true,
    "workbench.editor.tabSizing": "shrink",
    "search.exclude": {
        "**/.git": true,
        "**/node_modules": true,
    },
    "javascript.updateImportsOnFileMove.enabled": "never",
    "editor.tokenColorCustomizations": {
        "[Monokai]": {
            "comments": "#888",
            "textMateRules": [
                {
                    "scope": [
                        "meta.object-literal.key",
                    ],
                    "settings": {
                        "foreground": "#E6DB74"
                    }
                },
                {
                    "scope": [
                        "storage.type.js",
                        "support.type.property-name.css",
                        "support.type.property-name.json",
                    ],
                    "settings": {
                        "fontStyle": ""
                    }
                },
            ]
        }
    },
    "workbench.colorCustomizations": {
        "[Monokai]": {
            "editorGroupHeader.tabsBackground": "#000",
            "editor.background": "#121212",
            "sideBar.background": "#121212",
            "sideBar.foreground": "#ddd",
            "sideBar.border": "#643",
            "statusBar.background": "#282828",
            "statusBar.foreground": "#ccc",
            "sideBarSectionHeader.background": "#000"
        },
        "statusBar.background": "#005f5f",
        "statusBar.noFolderBackground": "#005f5f",
        "statusBar.debuggingBackground": "#005f5f"
    },
    "editor.fontSize": 15,
    "vim.statusBarColorControl": true,
    "workbench.statusBar.feedback.visible": false,
    "window.restoreFullscreen": true,
    "workbench.iconTheme": null,
    "workbench.tree.indent": 16,
    "explorer.autoReveal": false,
    "editor.hover.enabled": false,
    "editor.hover.sticky": false,
    "editor.showUnused": false,
    "editor.letterSpacing": 0.2,
    "editor.lightbulb.enabled": false,
    "files.trimTrailingWhitespace": true,
    "workbench.activityBar.visible": false,
    "editor.suggestSelection": "first",
    "explorer.decorations.badges": false,
    "explorer.decorations.colors": false,
    "update.showReleaseNotes": false,
    "terminal.external.osxExec": "iTerm.app",
    "workbench.tips.enabled": false,
    "workbench.editor.showTabs": false,
}